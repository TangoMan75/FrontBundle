{
	"name": "tangoman/front-bundle",
	"description": "Symfony Front Twig Extension Bundle",
	"version": "1.1.1",
	"type": "symfony-bundle",
	"keywords": [
		"Front",
		"Forms",
		"Buttons",
		"Menus",
		"Navbar",
		"Searchable",
		"Paginated",
		"Ordered",
		"List",
		"Back Office",
		"Responsive",
		"Bootstrap",
		"Symfony",
		"Twig",
		"Extension",
		"Service",
		"Bundle",
		"TangoMan"
	],
	"licence": "MIT",
	"authors": [
		{
			"name": "Matthias Morin",
			"email": "matthias.morin@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"twig/twig": "~1.12|~2",
		"tangoman/callback-bundle": "^2.0"
	},
	"scripts": {
		"post-install-cmd": [
			"php ../../../bin/console assets:install TangoManFrontBundle"
		]
	},
	"autoload": {
		"psr-4": {
			"TangoMan\\FrontBundle\\": ""
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
